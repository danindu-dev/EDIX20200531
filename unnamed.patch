--- BPMS/templates/BPMS/invo_view.html	(date 1589814355062)
+++ BPMS/templates/BPMS/invo_view.html	(date 1589814355062)
@@ -0,0 +1,48 @@
+{% extends 'BPMS/dashframe.html' %}
+
+{% block content %}
+        <div class="row " >
+          <div class="col-sm-12" >
+            <div class="card card-primary">
+                <div class="card-body">
+                    <table id="example1" class="table table-bordered table-striped dataTable dtr-inline" role="grid" aria-describedby="example1_info">
+                        <thead>
+                        <tr role="row">
+                        <th class="sorting_asc" tabindex="0" aria-controls="example1" rowspan="1" colspan="1" aria-sort="ascending" aria-label="Invoice Number: activate to sort column descending">
+                            Invoice Number
+                        </th>
+                        <th class="sorting" tabindex="0" aria-controls="example1" rowspan="1" colspan="1" aria-label="Invoice Date: activate to sort column ascending">
+                            Invoice Date
+                        </th>
+                        <th class="sorting" tabindex="0" aria-controls="example1" rowspan="1" colspan="1" aria-label="Item(s): activate to sort column ascending">
+                            Item(s)
+                        </th>
+                        <th class="sorting" tabindex="0" aria-controls="example1" rowspan="1" colspan="1" aria-label="Client Name: activate to sort column ascending">
+                            Client Name
+                        </th>
+                        <th class="sorting" tabindex="0" aria-controls="example1" rowspan="1" colspan="1" aria-label="Amount: activate to sort column ascending">
+                            Amount
+                        </th>
+                        </tr>
+                        </thead>
+                        <tbody>
+                        {% for obj,desc in invo_items.items %}
+                        <tr role="row" class="odd">
+                            <td tabindex="0" class="sorting_1"><a href="{% url 'invo_preview' obj.c_invoice_num|slice:str_slice%}">{{obj.c_invoice_num|slice:str_slice}}</a> </td>
+                            <td>{{obj.c_invoice_date}}</td>
+                            <td>{{desc}}</td>
+                            <td>{{obj.sub_client_id.sub_client_name}}</td>
+                            <td align="right">{{obj.c_total_amount|floatformat:2}}</td>
+                        </tr>
+                        {% endfor %}
+                        </tbody>
+                    </table>
+
+                </div>
+            </div>
+          </div>
+        </div>
+
+
+
+{% endblock %}
